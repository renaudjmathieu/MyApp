{
  "routes": [
    {
      "route": "/azurefunctions",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/reduxpage/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/d3/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/kaggle/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/pbi/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  }
}